<h1>Books</h1>

<table>
    <thead>
        <tr>
            <th></th>
            <th>Title</th>
            <th>Option</th>
        </tr>
    </thead>
    
    <tbody>
        
        <% @books.each do |book| %>
        
        <tr>
            <td><%= book.image %></td>
            <td><%= book.title %></td>
            <td><%= book.body %></td>
        </tr>
        <% end %>
    </tbody>
</table>